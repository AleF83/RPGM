{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "start:release": "node ./dist/index.js",
    "start:client": "react-scripts start",
    "start:server": "nodemon --exec babel-node -- server/index.js",
    "build": "concurrently \"yarn build:client\" \"yarn build:server\"",
    "build:client": "react-scripts build",
    "build:server": "babel server -d dist",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t alef83/rpgm-frontend .",
    "docker:start": "docker run --rm -p 80:3000 rpgm-frontend",
    "snyk": "snyk test"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001",
      "secure": false
    }
  },
  "dependencies": {
    "axios": "0.17.1",
    "emotion": "8.0.12",
    "express": "4.16.2",
    "history": "4.7.2",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-emotion": "8.0.12",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-redux": "next",
    "react-scripts": "1.1.0",
    "redux": "3.7.2",
    "redux-observable": "0.17.0",
    "rxjs": "5.5.6"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "concurrently": "3.5.1",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "nodemon": "1.14.11",
    "react-test-renderer": "16.2.0"
  }
}
