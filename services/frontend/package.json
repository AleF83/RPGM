{
	"name": "rpgm-frontend",
	"version": "0.0.1",
	"author": "AleF83",
	"license": "MIT",
	"private": false,
	"scripts": {
		"start": "yarn build && yarn start:server",
		"start:server": "node dist/server.js",
		"build": "yarn build:client && yarn build:server",
		"build:client": "webpack --config webpack.config.js",
		"build:server": "babel server.js -d dist",
		"dev:start": "concurrently \"yarn dev:start:server\" \"yarn dev:start:client\"",
		"dev:start:server": "nodemon --exec babel-node -- server.js",
		"dev:start:client": "webpack-dev-server",
		"docker:build": "docker build -t alef83/rpgm-frontend .",
		"docker:start": "docker run --rm -p 80:3000  rpgm-frontend",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"express": "^4.16.2",
		"history": "^4.7.2",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.6",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"react-router-redux": "^5.0.0-alpha.9",
		"redux": "^3.7.2"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.1",
		"babel-loader": "^7.1.2",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"case-sensitive-paths-webpack-plugin": "^2.1.1",
		"concurrently": "^3.5.1",
		"eslint": "^4.15.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.5.1",
		"html-webpack-inline-source-plugin": "^0.0.9",
		"html-webpack-plugin": "^2.30.1",
		"nodemon": "^1.14.11",
		"webpack": "^3.10.0",
		"webpack-dev-server": "^2.11.0"
	}
}
